- if(isAuthenticated)
	nav.main-nav(role="navigation")
		div.container(data-ng-controller="NavController as vm")
			div.navbar-header
				button.navbar-toggle(type="button" ng-click="vm.isCollapsed = !vm.isCollapsed")
					span.sr-only Toggle navigation
					span.icon-bar
					span.icon-bar
					span.icon-bar
			div.navbar-collapse.collapse(data-collapse="vm.isCollapsed")
				ul.nav.navbar-nav
					li.nav-item(ng-class="vm.getClass('')")
						a(href="/") Home
					- if(isAdmin)
							li.nav-item(ng-class="vm.getClass('/competencyLevels')")
								a(href="/#/competencyLevels") Competency Levels Admin
							li.nav-item(ng-class="vm.getClass('/objectiveAdmin')")
								a(href="/#/objectiveAdmin") Objectives Admin
					- if(directReports.length > 0 || isAdmin)
							li.nav-item(ng-class="vm.getClass('/rankings')")
								a(href="/#/rankings") Your Team
